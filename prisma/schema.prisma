generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model Session {
    id          String    @id @default(auto()) @map("_id") @db.ObjectId
    shop        String
    state       String
    isOnline    Boolean   @default(false)
    scope       String?
    expires     DateTime?
    accessToken String
    userId      BigInt?
}
